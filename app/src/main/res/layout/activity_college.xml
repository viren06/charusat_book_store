<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".college"
    android:background="@color/secondarylightblue">


    <androidx.gridlayout.widget.GridLayout
        android:layout_width="363dp"
        android:layout_height="535dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/idimageButtondepstar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/background"
            app:layout_column="0"
            app:layout_row="0"
            app:srcCompat="@drawable/depstar"
            tools:ignore="SpeakableTextPresentCheck" />


        <ImageButton
            android:id="@+id/idimageButtoncmpica"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/background"
            app:layout_column="0"
            app:layout_row="1"
            app:srcCompat="@drawable/cmpica"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/idimageButtonpdpias"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/background"
            app:layout_column="1"
            app:layout_row="0"
            app:srcCompat="@drawable/pdpias"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/idimageButtoncspit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/background"
            app:layout_column="1"
            app:layout_row="1"
            app:srcCompat="@drawable/cspit"
            tools:ignore="SpeakableTextPresentCheck" />


        <ImageButton
            android:id="@+id/idimageButtonrpcp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/background"
            app:layout_column="0"
            app:layout_row="2"
            app:srcCompat="@drawable/rpcp"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/idimageButtoni2im"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/background"
            app:layout_column="1"
            app:layout_row="2"
            app:srcCompat="@drawable/i2im"
            tools:ignore="SpeakableTextPresentCheck" />


    </androidx.gridlayout.widget.GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>